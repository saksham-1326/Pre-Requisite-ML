   "source": [
    "import jax\n",
    "import jax.numpy as jnum\n",
    "def f(x,y):\n",
    "    return (x**2)*y + (y**3)*jnum.sin(x)\n",
    "grad_f=jax.grad(f,argnums=(0,1))\n",
    "result=grad_f(1.0,2.0)\n",
    "x=result[0].item()\n",
    "y=result[1].item()\n",
    "print(\"Value of gradient at (1,2) is {0} and {1}\".format(x,y))"
   ]
  }
 ],
