   "source": [
    "import sympy as sp\n",
    "x,y= sp.symbols('x y')\n",
    "f=(x**2 * y) + (y**3 * sp.sin(x))\n",
    "df_dx=sp.diff(f,x)\n",
    "df_dy=sp.diff(f,y)\n",
    "xval=1.0\n",
    "yval=2.0\n",
    "grad_x=df_dx.evalf(subs={x:xval , y:yval})\n",
    "grad_y=df_dy.evalf(subs={x:xval , y:yval})\n",
    "print(grad_x)\n",
    "print(grad_y)"
   ]
  }
 ],
